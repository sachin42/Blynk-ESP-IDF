{
    "configurations": [
        {
            "name": "ESP-IDF",
            "compilerPath": "${config:idf.toolsPath}/tools/xtensa-esp-elf/esp-13.2.0_20240530/xtensa-esp-elf/bin/xtensa-esp32-elf-gcc",
            "compileCommands": "${config:idf.buildPath}/compile_commands.json",
            "includePath": [
                "${config:idf.espIdfPath}/components/**",
                "${config:idf.espIdfPathWin}/components/**",
                "${workspaceFolder}/**",
                "${workspaceFolder}/components/**",
                "${workspaceFolder}/components/Blynk/blynk-lib/Blynk/*",
                "${workspaceFolder}/components/Blynk/blynk-lib/utility/*",
                "${workspaceFolder}/components/Blynk/blynk-lib/*",
                "${workspaceFolder}/components/Blynk/include/*"
            ],
            "browse": {
                "path": [
                    "${config:idf.espIdfPath}/components",
                    "${config:idf.espIdfPathWin}/components",
                    "${workspaceFolder}",
                    "${workspaceFolder}/components/",
                    "${workspaceFolder}/components/Blynk/blynk-lib/Blynk",
                    "${workspaceFolder}/components/Blynk/blynk-lib/utility",
                    "${workspaceFolder}/components/Blynk/blynk-lib",
                    "${workspaceFolder}/components/Blynk/include"
                ],
                "limitSymbolsToIncludedHeaders": false
            }
        }
    ],
    "version": 4
}